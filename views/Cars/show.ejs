<%- include('../partials/html-head.ejs') %>
  <title>
    <%= car.year %>
      <%= car.make %>
        <%= car.model %>
  </title>
  </head>

  <body>
    <h1>
      <%= car.year %>
        <%= car.make %>
          <%= car.model %>
    </h1>
    <ul>
      <li>Car Color: <%= car.color %>
      </li>
      <% if (car.isAbleToStart) { %>
        <li>Does the engine start? Yes</li>
        <% } else {%>
          <li>Does the engine start? No</li>
          <% } %>
    </ul>

    <div>
      <form action="/cars/<%= car._id %>?_method=DELETE" method="POST">
        <button type="submit">Remove This Car</button>
      </form>
    </div>

    <div>
      <a href="/fruits/<%= car._id %>/edit">Edit Car Details</a>
    </div>

    <a href="/cars">Back To Home</a>
  </body>

  </html>